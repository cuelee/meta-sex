	Keywords = {research support, non-u.s. gov't;Mice, Inbred BALB C;Polymerase Chain Reaction;Animals;Oligonucleotide Probes;Base Sequence;comparative study;DNA;Kinetics;Liver;Species Specificity;Mice, Inbred C57BL;Apolipoprotein A-II;Crosses, Genetic;Mice, Inbred Strains;Polymorphism, Genetic;research support, u.s. gov't, p.h.s.;Protein Biosynthesis;Sequence Homology, Nucleic Acid;Mice;Amino Acid Sequence;Chromosome Mapping;Lipoproteins, HDL;Apolipoproteins A;Molecular Sequence Data},
	Month = {9},
